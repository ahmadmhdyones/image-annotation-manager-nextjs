{
  "name": "image-annotation-manager-nextjs",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Ahmad Yones",
    "email": "ahmadmhdyones@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadmhdyones/image-annotation-manager-nextjs"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "prod": "npm run build && npm run start",
    "clean": "rm -rf node_modules .next out dist",
    "restart:dev": "npm run clean && npm install && npm run dev",
    "restart:prod": "npm run clean && npm install && npm run prod",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md}\"",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run format && npm run lint",
    "fix": "npm run format:fix && npm run lint:fix"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom",
    "next": "$next"
  },
  "dependencies": {
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5"
  },
  "packageManager": "npm@11.0.0"
}
